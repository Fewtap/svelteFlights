{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch SvelteKit App",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "npm",
            "runtimeArgs": [
    "run",
    "dev"
],
            "outputCapture": "std",
            "env": {
                "DEBUG": "svelte:*,vite:*"
            },
            "console": "internalConsole",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}
